<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cis.co.kr.ciscultureinseoul">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cisicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cisicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JoinMemberActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileActivity"/>
        <activity android:name=".ForumWriteActivity" />
        <activity android:name=".ForumBodeActivity"/>
    </application>

</manifest>